{
  "name": "ytake/lom",
  "description": "lombok style code generator for php",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "yuuki takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "keywords": [
    "boilerplate",
    "application"
  ],
  "require": {
    "doctrine/annotations": "1.*",
    "nikic/php-parser": "4.*",
    "andrewsville/php-token-reflection": "1.*",
    "symfony/console": "^4.0",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "satooshi/php-coveralls": "2.0",
    "behat/behat": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\Lom\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/templates/"
    ]
  },
  "bin": [
    "bin/lom"
  ]
}
