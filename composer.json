{
  "name": "ytake/lom",
  "description": "lombok style code generator for php",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "yuuki takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "keywords": [
    "boilerplate",
    "application"
  ],
  "require": {
    "doctrine/annotations": "^1.2",
    "nikic/php-parser": "1.*",
    "andrewsville/php-token-reflection": "1.*",
    "symfony/console": "~2.3",
    "symfony/finder": "~2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "phpspec/phpspec": "^2.2",
    "satooshi/php-coveralls": "dev-master",
    "behat/behat": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\Lom\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/templates/DataAnnotation.php",
      "tests/templates/DataGetterSetterAnnotation.php",
      "tests/templates/InconsistencyConstructorAnnotation.php",
      "tests/templates/AllArgsConstructorExistsAnnotation.php",
      "tests/templates/GetterSetterAnnotation.php",
      "tests/templates/ValueAnnotation.php",
      "tests/templates/Testing.php",
      "tests/templates/AllArgsConstructorAnnotation.php",
      "tests/templates/InconsistencyConstructorAnnotation.php"
    ]
  },
  "bin": [
    "bin/lom"
  ]
}
