{
  "name": "ytake/lom",
  "description": "lombok style code generator for php",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "yuuki takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "keywords": [
    "boilerplate",
    "application"
  ],
  "require": {
    "php": "^7.1",
    "doctrine/annotations": "^1.2",
    "nikic/php-parser": "4.*",
    "andrewsville/php-token-reflection": "1.*",
    "symfony/console": "^4.0",
    "friendsofphp/php-cs-fixer": "^2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "satooshi/php-coveralls": "2.0",
    "sebastian/phpcpd": "*",
    "phploc/phploc": "*",
    "sensiolabs/security-checker": "^4.1.8"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\Lom\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/templates/"
    ]
  },
  "bin": [
    "bin/lom"
  ],
  "scripts": {
    "ci": [
      "./vendor/bin/phpunit",
      "./vendor/bin/phpcpd src/",
      "./vendor/bin/phploc src/ --log-xml=tests/build/phploc.xml"
    ],
    "security-checker": "./vendor/bin/security-checker security:check composer.lock"
  }
}
